<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <link rel="preconnect" href="https://fonts.googleapis.com" />
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
  <link
    href="https://fonts.googleapis.com/css2?family=PT+Sans+Caption:wght@400;700&display=swap"
    rel="stylesheet"
  />
  <style>
    body {
      margin: 0;
    }
    body,
    div,
    p,
    span,
    a {
      font-family: "PT Sans Caption", sans-serif !important ;
    }
    .wrapper {
      width: 960px;
      margin-left: auto;
      margin-right: auto;
      display: flex;
      display: -webkit-flex;
    }
    .content {
      width: calc(960px);
      height: auto;
      display: flex;
      display: -webkit-flex;
    }
    .content .rectangle-wrapper {
      background-color: #f0f0f0;
      display: flex;
      display: -webkit-flex;
      width: 100%;
      height: 78px;
    }
    .content .wrapper-dis-image {
      display: flex;
      display: -webkit-flex;
      margin-top: 64px;
      width: 90%;
      margin-left: auto;
      margin-right: auto;
    }
    .content .wrapper-dis-image .wrapper-avatar {
      width: 116px;
      height: 116px;
      border-radius: 50%;
      margin-top: auto;
      margin-bottom: auto;
    }
    .content .wrapper-dis-image .wrapper-avatar img {
      border-radius: 50%;
      width: 116px;
      height: 116px;
      border: 4px solid #f0f0f0;
    }
    .content .wrapper-details {
      display: flex;
      display: -webkit-flex;
    }
    .content .wrapper-context {
      display: flex;
      display: -webkit-flex;
    }
    .content .wrapper-context .left {
      width: 50%;
      padding-right: 24px;
    }
    .content .wrapper-context .right {
      width: 50%;
      padding-left: 24px;
      padding-right: 24px;
    }
    .fullname {
      padding-top: 6px;
      padding-left: 16px;
      color: #2e2725;
      font-weight: 700;
      font-size: 24px;
      margin: 0;
      text-align: left;
    }
    .jobtitle {
      padding-left: 16px;
      color: #2e2725;
      font-weight: 400;
      font-size: 16px;
      margin-top: 4px;
      margin-bottom: 0;
      text-align: left;
      font-family: "light-bold";
    }
    .divider {
      font-weight: 700;
      font-size: 16px;
      text-align: left;
      letter-spacing: 0.095em;
      text-transform: capitalize;
      background-color: #454545;
      color: #ffffff;
      height: 22px;
      width: 100%;
      padding-left: 10px;
      padding-right: 10px;
      line-height: 1.4;
    }
    .divider p {
      margin: 0;
    }
    .discretion {
      font-weight: 400;
      font-size: 16px;
      text-align: justify;
      color: #2e2725;
      line-height: 20px;
      margin: 0;
      font-family: "poppins-light";
    }
    .margin-top {
      margin-top: 71px;
    }
    .education {
      margin-top: 30px;
    }
    .education .wrapper-title-date {
      display: flex;
      display: -webkit-flex;
    }
    .education div p {
      margin-bottom: 4px;
      margin-top: 4px;
      font-family: "poppins-light";
    }
    .education .title {
      color: #2e2725;
      font-weight: 700;
      font-size: 16px;
      text-transform: capitalize;
      color: #2e2725;
    }
    .detail-item {
      color: #2e2725;
      margin-left: 24px;
      display: flex;
      display: -webkit-flex;
    }
    .detail-item .detail-item-data {
      padding-left: 12px;
    }
    .detail-item .detail-item-icon {
      margin-top: auto;
      margin-bottom: auto;
    }
    .detail-item .type {
      font-weight: 400;
      font-size: 16px;
      letter-spacing: 0.025em;
      margin: 0;
      font-family: "poppins-light";
    }
    .detail-item .value {
      font-weight: 700;
      font-size: 16px;
      color: #2e2725;
      margin-top: 4px;
      margin-bottom: 0;
      text-decoration: none;
    }
    .portfolio .image-wrapper {
      margin-left: 35px;
    }
    .portfolio img {
      width: 154px;
      height: 154px;
      margin-right: 10px;
      margin-top: 10px;
      border-radius: 8px;
      border: 1px solid #2e2725;
    }
    .portfolio .title {
      font-weight: 700;
      font-size: 16px;
      color: #2e2725;
    }
    .portfolio .image-link {
      font-family: "poppins-light";
      font-weight: 400;
      font-size: 12px;
      color: #2e2725;
    }
    .portfolio .image-link span {
      font-weight: 700;
      font-size: 12px;
      color: #2e2725;
    }
    .portfolio .video-link {
      font-family: "poppins-light";
      font-weight: 400;
      font-size: 12px;
      color: #2e2725;
    }
    .portfolio .video-link span {
      font-weight: 700;
      font-size: 12px;
      color: #2e2725;
    }
    .award {
      padding-top: 24px;
    }
    .award .wrapper-title-award {
      display: flex;
      display: -webkit-flex;
    }
    .award .wrapper-title-award .date {
      margin-left: auto;
    }
    .award .wrapper-title-award p {
      white-space: nowrap;
      margin-bottom: 0;
    }
    .award .wrapper-title-award .title {
      font-weight: 700;
      font-size: 16px;

      text-transform: capitalize;
      color: #2e2725;
    }
    .award .wrapper-text p {
      margin-top: 8px;
      margin-bottom: 0;
    }
    .pure-divider {
      margin-left: 24px;

      font-weight: 700;
      font-size: 24px;
      letter-spacing: 0.095em;
      text-transform: uppercase;
      color: #2e2725;
    }
    .progress-bar {
      margin-bottom: 48px;
    }
    .progress-bar p {
      margin-top: 0;
      margin-bottom: 0;
    }
    .progress-bar .title {
      font-weight: 700;
      font-size: 14px;
      color: #2e2725;
    }
    .progress-bar .text {
      font-weight: 400;
      font-size: 12px;
      line-height: 18px;
      font-family: "poppins-light";
      color: #2e2725;
      margin-left: auto;
    }
    .progress-bar .wrapper-detail {
      display: flex;
      display: -webkit-flex;
    }
    .progress-bar .wrapper-progress {
      margin-top: 16px;
      background: #f0f0f0;
      width: 100%;
      height: 8px;
      position: relative;
    }
    .progress-bar .wrapper-progress .progress {
      background-color: #454545;
      height: 8px;
    }
    .progress-bar .wrapper-progress .circle {
      position: absolute;
      background: #454545;
      border: 2px solid #ffffff;
      width: 14px;
      height: 14px;
      border-radius: 50%;
      top: -5px;
    }

    .course .title p {
      font-weight: 700;
      font-size: 16px;
      text-transform: capitalize;
      color: #2e2725;

      margin-bottom: 4px;
      margin-top: 0;
    }
    .course .date p {
      font-weight: 400;
      font-size: 12px;
      color: #2e2725;
      font-family: "poppins-light";
      margin-bottom: 4px;
      margin-top: 0;
    }
    .references p {
      margin-top: 0;
      margin-bottom: 4px;
    }
    .references .name p {
      font-weight: 700;
      font-size: 16px;
      text-transform: capitalize;

      color: #2e2725;
    }
    .references .company p {
      font-weight: 600;

      font-size: 12px;
      text-transform: capitalize;
      color: #2e2725;
    }
    .references .email a {
      font-family: "poppins-light";
      font-weight: 400;
      font-size: 12px;
      color: #2e2725;
      text-decoration: none;
    }
    .references .phone a {
      font-family: "poppins-light";
      font-weight: 400;
      font-size: 12px;
      color: #2e2725;
      text-decoration: none;
    }
    .socials {
      padding-top: 16px;
      display: -webkit-flex;
      display: flex;
      margin-left: auto;
      width: 238px;
    }
    .socials .item {
      width: 33%;
      margin-bottom: 36px;
      -webkit-filter: grayscale(100%);
      filter: grayscale(100%);
    }
    .socials .items img {
      width: 20px;
      height: 20px;
    }
  </style>
  <body>
