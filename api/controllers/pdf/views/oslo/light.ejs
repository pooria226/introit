              <% }); %>
              <!-- End Award -->
              <% } %>
            </div>
            <div class="right">
              <% if (cv.userExperiences.length > 0 ) { %>
              <!-- Start Margin -->
              <div class="margin-top"></div>
              <!-- End Margin -->

              <!-- Start Divider -->
              <div style="width: 100px">
                <div class="divider">
                  <p>Experience</p>
                </div>
              </div>

              <!-- End Divider -->

              <!-- Start Experience -->
              <% cv.userExperiences.forEach(function(item,index) { %>
              <div class="education">
                <div>
                  <p class="title"><%= item.institute %></p>
                </div>
                <div class="wrapper-title-date">
                  <div>
                    <p><%= item.country?.title %> <%= item.city?.title %></p>
                  </div>
                  <div style="margin-left: auto">
                    <p>
                      <%= moment(item.fromDate).format('MMM YYYY') %> - <%=
                      moment(item.toDate).format('MMM YYYY') %>
                    </p>
                  </div>
                </div>
                <div>
                  <p><%= item?.description %></p>
                </div>
              </div>
              <% }); %>
              <!-- End Experience -->
              <% } %> <% if (cv.userExpertises.length) { %>
              <!-- Start Margin -->
              <div style="margin-top: 64px"></div>
              <!-- End Margin -->

              <!-- Start Pure Divider -->
              <div style="width: 86.5px">
                <div class="divider">
                  <p>Expertise</p>
                </div>
              </div>

              <!-- End Pure Divider -->

              <!-- Start Margin -->
              <div style="margin-top: 40px"></div>
              <!-- End Margin -->

              <!-- Start Progress -->
              <% cv.userExpertises.forEach(function(item,index) { %>
              <div class="progress-bar">
                <div class="wrapper-detail">
                  <div class="title"><p><%= item?.title %></p></div>
                  <div class="text"><p><%= item.domination.title %></p></div>
                </div>
                <div class="wrapper-progress">
                  <div
                    style="width: <%= item.percentage %>%"
                    class="progress"
                  ></div>
                  <div
                    style="left: <%= item.percentage - 2 %>%"
                    class="circle"
                  ></div>
                </div>
              </div>
              <% }); %>
              <!-- End Progress -->
              <% } %> <% if (cv.userExtraActivities.length > 0 ) { %>
              <!-- Start Margin -->
              <div class="margin-top"></div>
              <!-- End Margin -->

              <!-- Start Divider -->
              <div style="width: 242px">
                <div class="divider">
                  <p>Extra-curricular Activities</p>
                </div>
              </div>
              <!-- End Divider -->

              <!-- Start Extra Activities -->
              <% cv.userExtraActivities.forEach(function(item,index) { %>
              <div>
                <div class="education">
                  <div>
                    <p class="title"><%= item?.title %></p>
                  </div>
                  <div class="wrapper-title-date">
                    <div>
                      <p><%= item.country?.title %> <%= item.city?.title %></p>
                    </div>
                    <div style="margin-left: auto">
                      <p>
                        <%= moment(item.fromDate).format('MMM YYYY') %> - <%=
                        moment(item.toDate).format('MMM YYYY') %>
                      </p>
                    </div>
                  </div>

                  <div>
                    <p><%= item?.description %></p>
                  </div>
                </div>
              </div>
              <% }); %>
              <!-- End Extra Activities -->

              <% } %> <% if (cv.userInterships.length) { %>

              <!-- Start Margin -->
              <div style="margin-top: 40px"></div>
              <!-- End Margin -->

              <!-- Start Pure Divider -->
              <div style="width: 106px">
                <div class="divider">
                  <p>Internships</p>
                </div>
              </div>

              <!-- End Pure Divider -->

              <!-- Start Margin -->
              <div style="margin-top: 40px"></div>
              <!-- End Margin -->

              <!-- Start Internships -->
              <% cv.userInterships.forEach(function(item,index) { %>
              <div class="course">
                <div class="title"><p><%= item?.title %></p></div>

                <div class="date">
                  <p><%= item.country?.title %> <%= item.city?.title %></p>
                </div>
                <div class="date">
                  <p>
                    <%= moment(item.fromDate).format('MMM YYYY') %> - <%=
                    moment(item.toDate).format('MMM YYYY') %>
                  </p>
                </div>
              </div>
              <!-- Start Margin -->
              <div style="margin-top: 16px"></div>
              <!-- End Margin -->
              <% }); %>
              <!-- End Internships -->
              <% } %> <% if (cv.hobbies ) { %>
              <!-- Start Margin -->
              <div class="margin-top"></div>
              <!-- End Margin -->

              <!-- Start Divider -->
              <div style="width: 75px">
                <div class="divider">
                  <p>Hobbies</p>
                </div>
              </div>

              <!-- End Divider -->

              <!-- Start Hobbies -->

              <div class="discretion">
                <p><%= cv?.hobbies %></p>
              </div>

              <!-- End Hobbbies -->

              <% } %> <% if (cv.publications ) { %>
              <!-- Start Margin -->
              <div class="margin-top"></div>
              <!-- End Margin -->

              <!-- Start Divider -->
              <div style="width: 115px">
                <div class="divider">
                  <p>Publications</p>
                </div>
              </div>
              <!-- End Divider -->

              <!-- Start Publications -->

              <div class="discretion">
                <p><%= cv?.publications %></p>
              </div>

              <!-- End Publications -->
              <% } %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
