                        <rect width="16" height="16" fill="white" />
                      </clipPath>
                    </defs>
                  </svg>
                </div>
              </div>

              <div class="step-content">
                <!-- Start Divider -->
                <div class="divider">
                  <p>Education</p>
                </div>
                <!-- End Divider -->
                <!-- Start Education -->
                <% cv.userEducations.forEach(function(item,index) { %>
                <div class="relative">
                  <div class="education">
                    <div class="dot"></div>
                    <div class="date-relative">
                      <p>
                        <%= moment(item.fromDate).format('YYYY MM') %> <br />
                        - <%= moment(item.toDate).format('YYYY MM') %>
                      </p>
                    </div>
                    <div class="wrapper-title-date">
                      <div>
                        <p class="title"><%= item.institute %></p>
                      </div>
                      <div style="margin-left: auto">
                        <p>
                          <%= item.country?.title %> <%= item.city?.title %>
                        </p>
                      </div>
                    </div>

                    <div>
                      <p><%= item?.degree?.title %> , <%= item?.major %></p>
                    </div>
                  </div>
