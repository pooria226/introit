<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <link rel="preconnect" href="https://fonts.googleapis.com" />
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
  <link
    href="https://fonts.googleapis.com/css2?family=PT+Sans+Caption:wght@400;700&display=swap"
    rel="stylesheet"
  />
  <style>
    body {
      margin: 0;
    }
    body,
    div,
    p,
    span,
    a {
      font-family: "PT Sans Caption", sans-serif !important ;
    }

    .wrapper {
      width: 960px;
      margin-left: auto;
      margin-right: auto;
      display: -webkit-flex;
      display: flex;
    }
    .content {
      width: calc(960px - 257px);
      margin-right: 24px;
      height: auto;
      display: -webkit-flex;
      display: flex;
    }
    .sidebar {
      width: 257px;
    }
    .wrapper-fullname {
      display: -webkit-flex;
      display: flex;
    }
    .wrapper-fullname .circle {
      width: 80px;
      height: 80px;
      background-color: #373d48;
      border-radius: 50%;
      position: relative;
    }
    .wrapper-fullname .circle p {
      font-weight: 400;
      font-size: 16px;
      letter-spacing: 0.01em;
      text-transform: uppercase;
      color: #ffffff;
    }
    .wrapper-fullname .circle .text {
      position: absolute;
      right: 18px;
      bottom: 5px;
      display: -webkit-flex;
      display: flex;
    }
    .fullname {
      color: #373d48;
      font-weight: 700;
      font-size: 24px;
      padding-left: 18px;
      margin-bottom: 0;
      text-align: center;

      text-transform: uppercase;
    }
    .fullname p {
      margin: 0;
      text-align: left;
    }
    .jobtitle {
      color: #373d48;
      font-weight: 400;
      font-size: 14px;
      margin-top: 4px;
      margin-bottom: 0;
      text-align: left;
      font-family: "light-bold";
      text-transform: capitalize;
    }

    .divider {
      font-weight: 700;
      font-size: 20px;
      text-align: left;
      letter-spacing: 0.095em;
      text-transform: uppercase;
      color: #373d48;
      height: 33px;
    }
    .sidebar .divider {
      font-size: 16px !important;
    }
    .divider p {
      margin: 0;
    }
    .discretion {
      font-weight: 400;
      font-size: 14px;
      text-align: justify;
      color: #373d48;
      line-height: 20px;
      margin-top: 0px;
      font-family: "poppins-light";
    }
    .margin-top {
      margin-top: 71px;
    }
    .education {
      margin-top: 14px;
      font-size: 14px;
    }
    .education .wrapper-title-date {
      display: -webkit-flex;
      display: flex;
    }
    .education div p {
      margin-bottom: 4px;
      margin-top: 4px;
      font-family: "poppins-light";
    }
    .education .title {
      color: #373d48;
      font-weight: 700;
      font-size: 16px;
      text-transform: capitalize;
      color: #373d48;
    }
    .detail-item {
      color: #373d48;
    }
    .detail-item .type {
      font-weight: 400;
      font-size: 12px;
      letter-spacing: 0.025em;
      margin: 0;
      font-family: "poppins-light";
    }
    .detail-item .value {
      font-weight: 700;
      font-size: 12px;
      color: #373d48;
      margin-top: 4px;
      margin-bottom: 0;
      text-decoration: none;
    }
    .portfolio .image-wrapper {
      margin-left: 35px;
    }
    .portfolio img {
      width: 143px;
      height: 143px;
      margin-right: 10px;
      margin-top: 10px;
      border-radius: 8px;
      border: 1px solid #373d48;
    }
    .portfolio .title {
      font-weight: 700;
      font-size: 16px;
      color: #373d48;
    }
    .portfolio .image-link {
      font-family: "poppins-light";
      font-weight: 400;
      font-size: 12px;
      color: #373d48;
    }
    .portfolio .image-link span {
      font-weight: 700;
      font-size: 12px;
      color: #373d48;
    }
    .portfolio .video-link {
      font-family: "poppins-light";
      font-weight: 400;
      font-size: 12px;
      color: #373d48;
    }
    .portfolio .video-link span {
      font-weight: 700;
      font-size: 12px;
      color: #373d48;
    }
    .award {
      margin-top: 24px;
    }
    .award .wrapper-title-award {
      display: -webkit-flex;
      display: flex;
    }
    .award .wrapper-title-award .date {
      margin-left: auto;
    }
    .award .wrapper-title-award p {
      white-space: nowrap;
      margin: 0;
    }
    .award .wrapper-title-award .title {
      font-weight: 700;
      font-size: 16px;

      text-transform: capitalize;
      color: #373d48;
    }
    .award .wrapper-text p {
      margin-top: 8px;
      font-family: "poppins-light";
      margin-bottom: 0;
      font-size: 14px;
    }
    .pure-divider {
      margin-left: 24px;

      font-weight: 700;
      font-size: 24px;
      letter-spacing: 0.095em;
      text-transform: uppercase;
      color: #373d48;
    }
    .progress-bar {
      margin-bottom: 16px;
    }
    .progress-bar p {
      margin-top: 0;
      margin-bottom: 0;
    }
    .progress-bar .title p {
      font-weight: 700;
      font-size: 14px;
      color: #373d48;
      font-family: "poppins-bold" !important;
    }
    .progress-bar p {
      font-weight: 400;
      font-size: 12px;
      line-height: 18px;
      font-family: "poppins-light";
      color: #373d48;
    }
    .progress-bar .text {
      font-weight: 400;
      font-size: 12px;
      line-height: 18px;
      font-family: "poppins-light";
      color: #373d48;
      margin-left: auto;
    }
    .progress-bar .wrapper-detail {
      display: -webkit-flex;
      display: flex;
    }
    .progress-bar .wrapper-progress {
      margin-top: 4px;
      width: 100%;
      border-radius: 8px;
      position: relative;
      display: -webkit-flex;
      display: flex;
    }
    .progress-bar .wrapper-progress .progress {
      background-color: #9bdae9;
      height: 8px;
      border-radius: 8px;
    }
    .progress-bar .wrapper-progress .circle {
      position: absolute;
      background: #9bdae9;
      border: 2px solid #ffffff;
      width: 14px;
      height: 14px;
      border-radius: 50%;
      top: -5px;
    }

    .course .title p {
      font-weight: 700;
      font-size: 16px;
      text-transform: capitalize;
      color: #373d48;

      margin-bottom: 4px;
      margin-top: 0;
    }
    .course .date p {
      font-weight: 400;
      font-size: 12px;
      color: #373d48;
      font-family: "poppins-light";
      margin-bottom: 4px;
      margin-top: 0;
    }

    .references p {
      margin-top: 0;
      margin-bottom: 4px;
    }
    .references .name p {
      font-weight: 700;
      font-size: 16px;
      text-transform: capitalize;

      color: #373d48;
    }
    .references .company p {
      font-weight: 600;

      font-size: 12px;
      text-transform: capitalize;
      color: #373d48;
    }
    .references .email a {
      font-family: "poppins-light";
      font-weight: 400;
      font-size: 12px;
      color: #373d48;
      text-decoration: none;
    }
    .references .phone a {
      font-family: "poppins-light";
      font-weight: 400;
      font-size: 12px;
      color: #373d48;
      text-decoration: none;
    }
    .socials {
      display: -webkit-flex;
      display: flex;
      width: 150px;
      flex-wrap: wrap;
      margin-left: auto;
      margin-right: auto;
    }
    .socials .item {
      width: 33%;
      margin-bottom: 36px;
    }
    .socials .items img {
      width: 20px;
      height: 20px;
    }
    .step {
      display: -webkit-flex;
      display: flex;
      position: relative;
    }
    .step .divider {
      position: relative;
      top: -2px;
    }
    .step .icon {
      background-color: #373d48;
      width: 26px;
      height: 26px;
      border-radius: 50%;
      text-align: center;
    }
    .step .icon svg {
      position: relative;
      top: 5px;
      left: 0.5px;
    }
    .step .line {
      background: #d5d6d6;
      width: 2px;
      height: 100%;
      position: absolute;
      top: 26px;
      left: 10px;
    }
    .step .step-content {
      padding-left: 12px;
      width: 100%;
    }
    .step .step-content .relative {
      position: relative;
    }
    .step .step-content .relative:last-child {
      padding-bottom: 24px;
    }
    .step .date-relative {
      position: absolute;
      left: -90px;
      top: -8px;
    }
    .step .date-relative p {
      font-size: 12px;
    }
    .step .dot {
      background-color: #373d48;
      width: 8px;
      height: 8px;
      border-radius: 50%;
      position: absolute;
      left: -28.5px;
      top: 7px;
    }
    .divider-icon {
      display: -webkit-flex;
      display: flex;
    }
    .divider-icon .icon {
      padding-right: 8px;
    }
  </style>
  <body>
    <div class="wrapper">
      <div class="content">
        <div>
          <div>
