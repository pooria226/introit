                <div class="divider">
                  <p>About Me</p>
                </div>
                <!-- End Divider -->
                <div class="relative">
                  <div class="dot"></div>
                  <p class="discretion">
                    <%- cv.description ? cv.description : '' %>
                  </p>
                </div>
              </div>
            </div>
            <% } %> <% if (cv.userEducations.length > 0 ) { %>
